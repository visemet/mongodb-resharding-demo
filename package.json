{
  "name": "@visemet/mongodb-resharding-demo",
  "version": "0.0.1",
  "description": "MongoDB.live demo for the resharding session",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "mongodb",
    "resharding",
    "reshardCollection"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npx gts lint",
    "clean": "npx gts clean",
    "compile": "npx tsc",
    "fix": "npx gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "monitor": "npx ts-node --require dotenv-safe/config -- ./src/main/monitor.ts",
    "populate-data": "npx ts-node --require dotenv-safe/config -- ./src/main/populate.ts",
    "apply-load": "npx ts-node --require dotenv-safe/config -- ./src/main/liveupdate.ts"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.0",
    "@types/faker": "^5.5.5",
    "@types/mongodb": "^3.6.12",
    "@types/node": "^14.11.2",
    "dotenv-safe": "^8.2.0",
    "gts": "^3.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "bytes": "^3.1.0",
    "faker": "^5.5.3",
    "log-update": "^4.0.0",
    "mongodb": "^3.6.6",
    "random-js": "^2.1.0"
  }
}
